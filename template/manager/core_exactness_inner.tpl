[[ for key,quer in querys]]	<tr>		<td>			<a href="javascript: void(0);" onclick="deleteRow(this,'{quer['query']}');" ><img src="/images/icon-clear.png" alt="удалить" title="удалить" style="margin: 6px 2px 0 0;" /></a> 			<a target="_blank" href="http://yandex.ru/yandsearch?text={df('htmlspecialchars',quer['query'])}&lr=213&stpar2;=%2Fh1%2Ftm2%2Fs2&stpar4;=%2Fs2&stpar1;=%2Fu0" ><span id="word{key}" >{quer['query']}</span></a>			<input type="hidden" name="mas[{key+startId}][query]" value="{quer['query']}" />			<input type="hidden" name="mas[{key+startId}][count]" value="{quer['count']}" />		</td>		<td>			{quer['count']}		</td>	</tr>		<script>		[[ set startId = df('sizeof',querys) + startId]];		startId = {startId};	</script>[[endfor]]